<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_workcenter_variant_check_wizard" model="ir.ui.view">
        <field name="name">workcenter.variant.check.wizard.form</field>
        <field name="model">workcenter.variant.check.wizard</field>
        <field name="arch" type="xml">
            <form string="Select Product Templates">
                <group>
                    <field name="product_template_ids" widget="many2many_tags"/>
                </group>
                <footer>
                    <button string="Check Variants" type="object"
                            name="action_submit_variant_check" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>